! Rofi colors.
rofi.color-window: #261B18, #261B18, #E08A49
rofi.color-normal: #261B18, #FFFFFF, #261B18, #E08A49, #261B18
rofi.color-active: #261B18, #FFFFFF, #261B18, #E08A49, #261B18
